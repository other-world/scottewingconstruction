#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jcummings/Documents/Work/SpringMicro/SpringMicroHost1/scottewingconstruction/node_modules/.pnpm/astro@5.7.13_@types+node@22.15.17_rollup@4.40.2_typescript@5.8.3/node_modules/astro/node_modules:/Users/jcummings/Documents/Work/SpringMicro/SpringMicroHost1/scottewingconstruction/node_modules/.pnpm/astro@5.7.13_@types+node@22.15.17_rollup@4.40.2_typescript@5.8.3/node_modules:/Users/jcummings/Documents/Work/SpringMicro/SpringMicroHost1/scottewingconstruction/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jcummings/Documents/Work/SpringMicro/SpringMicroHost1/scottewingconstruction/node_modules/.pnpm/astro@5.7.13_@types+node@22.15.17_rollup@4.40.2_typescript@5.8.3/node_modules/astro/node_modules:/Users/jcummings/Documents/Work/SpringMicro/SpringMicroHost1/scottewingconstruction/node_modules/.pnpm/astro@5.7.13_@types+node@22.15.17_rollup@4.40.2_typescript@5.8.3/node_modules:/Users/jcummings/Documents/Work/SpringMicro/SpringMicroHost1/scottewingconstruction/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
